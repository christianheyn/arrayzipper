{
  "name": "arrayzipper",
  "version": "0.0.1",
  "description": "zip two or more arrays to one",
  "main": "dist/app.js",
  "scripts": {
    "test": "npm run lint:js && npm run lint:js:test && node test/dist/js/test.js",
    "postinstall": "npm run browserify && npm run browserify:test && npm test",
    "browserify": "browserify js/app.js -o dist/js/app.js -t [ babelify --presets [ es2015 ] ]",
    "browserify:test": "browserify test/js/test.js -o test/dist/js/test.js -t [ babelify --presets [ es2015 ] ]",
    "watch:js": "watch-run -p 'js/*.js' npm run browserify",
    "watch:js:test": "watch-run -p 'test/js/**/*.js' npm run browserify:test",
    "watch:all": "npm run watch:js & npm run watch:js:test",
    "lint:js": "eslint js/**/*.js",
    "lint:js:test": "eslint test/js/**/*.js",
    "commit": "git-cz"
  },
  "author": "Christian Heyn",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/christianheyn/arrayzipper.git"
  },
  "keywords": [
    "zipper",
    "concat",
    "array"
  ],
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babelify": "^7.3.0",
    "browserify": "^13.0.0",
    "commitizen": "^2.8.2",
    "cz-conventional-changelog": "^1.1.6",
    "eslint": "^2.12.0",
    "watch-run": "^1.2.4",
    "tape": "^4.6.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
